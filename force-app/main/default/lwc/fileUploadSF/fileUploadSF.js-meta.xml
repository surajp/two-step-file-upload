<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>55.0</apiVersion>
    <isExposed>true</isExposed>
    <targets>
      <target>lightning__FlowScreen</target>
    </targets>
    <targetConfigs>
      <targetConfig targets="lightning__FlowScreen">
      	<property name="title" type="string" default="FlowFileUpload.txt" label="File Name with extension" role="inputOnly"/>
      	<property name="fileContents" type="string" default="" label="File Contents (base 64 string)" role="inputOnly" />
      	<property name="fileId" type="string" default="" label="Unique identifier to retrieve file from local storage" role="inputOnly"/>
      	<property name="recordId" type="string" default="" label="Id of the record to which the file should be uploaded" role="inputOnly"/>
      </targetConfig>
    </targetConfigs>
</LightningComponentBundle>
